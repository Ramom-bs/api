<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, inital-scale=1.0">
    <title>Pagina-dinamica</title>
</head>

<body onload="pagina-dinamica">

    <script>
        const h1 = document.createElement('h1');
        const h2 = document.createElement('h2');
        const p1 = document.createElement('p1');
        document.body.appendChild(h1).innerHTML = "Pagina-dinamica"
        document.body.appendChild(h2).innerHTML = "dinamico"
        document.body.appendChild(p1).innerHTML = "criando pagina javascript"
    </script>

</body>

</html>